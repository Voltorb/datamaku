	score=0
	redscore=0
	bluescore=0
	greenscore=100
end

function start_music(x)
	if (not music_on) then
		music(x)
		music_on = true
	end
end

function stop_music()
	if (music_on) then
		music(-1)
		music_on = false
	end
end
		

