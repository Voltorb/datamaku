end
		
function game_update()
	start_music(0)
	moveship()
	colorswitch()
	new_datagram()
	foreach(datagram,move_datagram)
	foreach(datagram,hit_datagram)
 foreach(datagram,remove_datagram)
 green_state()
 gameover()
end


function game_draw()
 cls()
	map(0,0,0,0,16,16)
	lowerdisplay()
 upperdisplay()
 drawship()
 foreach(datagram,draw_datagram)
 leftbar()
 rightbar()

end


function _update()
	if state==0 then
		title_menu()
	elseif state==1 then
		instructions()
	elseif state==2 then
		instructions_pt2()
	elseif state==3 then
		instructions_pt3()
	elseif state==4 then
		instructions_pt4()
	elseif state==5 then
		game_update()
	elseif state==6 then
		gameover_screen()
	end
end

function _draw()
