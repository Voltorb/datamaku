 	title_draw()
 elseif state==1 then
 	instructions_draw()
 elseif state==2 then
 	instructions_draw_pt2()
 elseif state==3 then
 	instructions_draw_pt3()
	elseif state==4 then
 	instructions_draw_pt4()
	elseif state==5 then
 	game_draw()
	elseif state==6 then
		gameover_draw()
	end
end
