function drawship()
	if greenstate == 1 then
	 spr(ship.col+9,ship.x,ship.y)
	else
		spr(ship.col+6,ship.x,ship.y)
	end
end

function moveship()
	if btn(0) then
		ship.x -= ship.spd
	end
	
	if btn(1) then
		ship.x += ship.spd
	end
	
	if btn(2) then
		ship.y -= ship.spd
	end
	
	if btn(3) then
		ship.y += ship.spd
	end
	
	if ship.x <9 then
		ship.x = 9
	end
	
	if ship.x >112 then
		ship.x = 112
	end
	
	if ship.y <15 then
		ship.y = 15
	end
	
	if ship.y >104 then
		ship.y = 104
	end
end

function colorswitch()
	if btnp(4) then
		ship.col = -1*ship.col
		sfx(0)
	end
end



function create_datagram()
	local dgram = {}
	dgram.side=flr(rnd(4))+1
	--1 = left
	if dgram.side == 1 then
  dgram.x = 7	
		dgram.y = flr(rnd(98))+15
	end
	
	--2 = right
	if dgram.side == 2 then
  dgram.x = 120	
		dgram.y = flr(rnd(98))+15
	end
	
	--3 = top
	if dgram.side == 3 then
  dgram.x = flr(rnd(113))+7
		dgram.y = 15
	end
	
	--4 = bottom
	if dgram.side == 4 then
  dgram.x = flr(rnd(113))+7
		dgram.y = 113
	end
	
	dgram.size = 1
	col={}
	col[1]=8
	col[2]=12
	col[3]=11
	dgram.col = col[flr(rnd(2))+1]
	--sometimes, datagram is green
	if	flr(rnd(50))+1 == 1 
 or greenstate==1 then
	dgram.col = 11
	end
	
	
	--this directs the grams
	--at the top left of sprite.
	--although "wrong", this plays
	--better than when aimed
	--directly at ship.
	--when in greenstate, grams
	--fire directly at ship
	local dir_tune=3*greenstate
	dgram.xdir = (ship.x+dir_tune) - dgram.x
 dgram.ydir = (ship.y+dir_tune) - dgram.y
 
	add(datagram,dgram)
	--red score increases if dgram is red
	if dgram.col == 8 
	and ship.col == 1 then
		redscore += 1
	end
	
	if dgram.col == 11
	and greenstate == 1 then
		greenscore -= 2
		score += 2*level
	end
	
	return dgram
end

function move_datagram(dgram)
 --datagrams speed is function
 --of initial distance from 
 --ship. this works quite well,
 --prevents cheap deaths. 
 local dgramscaling = max(34-(2*level)-greenstate,10)
 dgram.x += dgram.xdir/dgramscaling
 dgram.y += dgram.ydir/dgramscaling
end

--spawn datagram
function new_datagram()
	if flr(rnd(7)) == 5 then 
		create_datagram()
	end
end
		

--handle collision
--between ship and datagram
function hit_datagram(dgram)
 if abs(dgram.x-(ship.x+5)) < 4
 and abs(dgram.y-(ship.y+4)) < 4 then
 	
		--scoring
 	if ship.col == 1
 	and dgram.col == 12 then
 		bluescore += 3
 		score += level
 	elseif ship.col == 1
 	and dgram.col == 8 then
 		redscore += 3
 	elseif ship.col == -1
 	and dgram.col == 8 then
 		redscore -= 5
 	elseif ship.col == -1
 	and dgram.col == 12 then
 		bluescore -= 3 
 		score -= 2*level
 	elseif dgram.col == 11 then
 		redscore += 10
 	end
 	
 	del(datagram,dgram)
 end
 
 if score <= 0 then
 	score = 0
 end
 
 if score >= 32000 then
 	score = 32000
 end
 
 if bluescore <= 0 then
 	bluescore = 0
 end
 
 if redscore <= 0 then
 	redscore = 0
 end
 
 if bluescore >= 100 then
 	bluescore = 100
 end
 
 if redscore >= 100 then
 	redscore = 100
 end
end


--if datagram leaves screen, delete
function remove_datagram(dgram)
 if dgram.x > 120
 or dgram.x < 7
 or dgram.y > 113
 or dgram.y < 15 then
 del(datagram,dgram)
 end
end
 
function draw_datagram(dgram)
	circfill(dgram.x,dgram.y,dgram.size,dgram.col)
end


function leftbar()
	rectfill(2,89-redscore/2,5,89,8)
end

function rightbar()
	if greenstate == 0 then
		rectfill(122,89-bluescore/2,125,89,12)
	elseif greenstate == 1 then
	rectfill(122,89-greenscore/2,125,89,11)
 end
end

function green_state()
	if bluescore == 100 then
		greenstate=1
		bluescore = 0
  greenscore=100
  pal(12,11)
 	pal(2,12)	
	end
	
	if greenstate == 1
	and greenscore <= 0 then
		greenstate=0
		bluescore = 0
		pal()
		score += 300*level
		level+=1
		sfx(1)
	end
end
	
function lowerdisplay()
	if redscore >= 70 then
		print('danger',52,118,8)
	elseif greenstate == 1 then
		print('upload',52,118,11)
	elseif level <= 9 then
		print("zone"..level,54,118,6)
	elseif level >= 10 then
		print("zone"..level,52,118,6)
	end
end	


function upperdisplay()
	if score >= 32000 then
		print("max",58,6,6)
	else
		print(score,54,6,6)
	end
end
	
function gameover()
 if redscore == 100 then
 	stop_music()
 	state=6
 end
end

--glitching
--credit to impbox (picoracer)
function glitch()
	local source = rnd(flr(0x6000+8192))
	local range = flr(rnd(1000))			
	local dest = 0x6000 + rnd(8192-range)-2
	memcpy(dest,source,range)
end	

function update_hiscore()
	if score > hiscore then
		hiscore = score
	end
end

function gameover_screen()
	start_music(2)
	update_hiscore()
	if btnp(4) then
		stop_music()
		state=5
		game_init()
		ship.col = 1
	end
	if btnp(5) then
		stop_music()
		state=0
		game_init()
	end
end

